{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/img/(.*)",
      "dest": "/public/img/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    }
  ]
} 